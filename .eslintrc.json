{
    "env": {
      "browser": true,
      "es2021": true
    },
    
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "quotes": 0,
      "no-multiple-empty-lines": 1,
      "no-multi-spaces": 1,
      "no-mixed-spaces-and-tabs": 1,
      "no-extra-parens": 1,
      "no-trailing-spaces": 1,
      "no-unused-vars": 1,
      "consistent-return": 1,
      "linebreak-style": 1,
      "prettier/prettier": [2, { "endOfLine": "auto" }],
      "import/extensions": [
        2,
        "ignorePackages",
        {
          "js": "always"
        }
      ]
    },
    "plugins": ["prettier"]
  }
  